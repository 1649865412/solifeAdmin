<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.cartmatic.estore.common.model.order.OrderReturn"
        table="order_return"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="orderReturnId"
            column="orderReturnId"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
 		<version
            name="version"
            type="java.lang.Integer"
            column="version"
            access="property"
            unsaved-value="undefined"
        />
        <property
            name="rmaNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="rmaNo"
            length="32"
            not-null="true"
            unique="false"
        />
        <property
            name="exchangeOrderId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="exchangeOrderId"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="returnType"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="returnType"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="status"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="status"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="isPhysicalReturn"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isPhysicalReturn"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="itemSubTotal"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="itemSubTotal"
            precision="12"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="itemTax"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="itemTax"
            precision="12"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="shippingTax"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="shippingTax"
            precision="12"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="shippingCost"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="shippingCost"
            precision="12"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="discountAmount"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="discountAmount"
            precision="12"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="lessRestockAmount"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="lessRestockAmount"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="note"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="note"
            length="1024"
            not-null="false"
            unique="false"
        />
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createTime"
            length="0"
            not-null="true"
            unique="false"
        />
        <property
            name="createBy"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="createBy"
            length="10"
            not-null="true"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to SalesOrder -->
		<many-to-one
			name="salesOrder"
			class="com.cartmatic.estore.common.model.order.SalesOrder"
            not-null="false"
			fetch="select"
		>	
			<column name="salesOrderId" />
		</many-to-one>
    <!-- imported bi-directional many-to-one association to AppUser -->
		<many-to-one
			name="appUser"
			class="com.cartmatic.estore.common.model.system.AppUser"
            not-null="false"
			fetch="select"
		>	
			<column name="receivedBy" />
		</many-to-one>
 <!-- exported bi-directional one-to-many association to OrderReturnSku -->
	<set name="orderReturnSkus"
	     table="order_return_sku"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="orderReturnId"/>
		</key>
		<one-to-many
			class="com.cartmatic.estore.common.model.order.OrderReturnSku"
		/>
	</set>     
    </class>
</hibernate-mapping>
