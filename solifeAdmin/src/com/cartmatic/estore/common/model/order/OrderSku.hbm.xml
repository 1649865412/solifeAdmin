<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.cartmatic.estore.common.model.order.OrderSku"
        table="order_sku"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="orderSkuId"
            column="orderSkuId"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
 		<version
            name="version"
            type="java.lang.Integer"
            column="version"
            access="property"
            unsaved-value="undefined"
        />
         <property
            name="productId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="productId"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="productName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="productName"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="productSkuCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="productSkuCode"
            length="32"
            not-null="false"
            unique="false"
        />
        <property
            name="itemType"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="itemType"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="quantity"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="quantity"
            length="10"
            not-null="true"
            unique="false"
        />
        <property
            name="price"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="price"
            precision="12"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="discountPrice"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="discountPrice"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="costPrice"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="costPrice"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="isOnSale"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isOnSale"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="isWholesale"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isWholesale"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="tax"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="tax"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="taxName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="taxName"
            length="64"
            not-null="false"
            unique="false"
        />
        <property
            name="itemDiscountAmount"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="itemDiscountAmount"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="weight"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="weight"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="allocatedQuantity"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="allocatedQuantity"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="allocatedPreQty"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="allocatedPreQty"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="displaySkuOptions"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="displaySkuOptions"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="accessories"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="accessories"
            length="512"
            not-null="false"
            unique="false"
            
        />
     <!-- imported bi-directional many-to-one association to GiftCertificate -->
		<many-to-one
			name="giftCertificate"
			class="com.cartmatic.estore.common.model.sales.GiftCertificate"
            not-null="false"
			fetch="select"
		>	
			<column name="giftCertificateId" />
		</many-to-one>
    <!-- imported bi-directional many-to-one association to ProductSku -->
		<many-to-one
			name="productSku"
			class="com.cartmatic.estore.common.model.catalog.ProductSku"
            not-null="false"
			fetch="select"
		>	
			<column name="productSkuId" />
		</many-to-one>
    <!-- imported bi-directional many-to-one association to OrderShipment -->
		<many-to-one
			name="orderShipment"
			class="com.cartmatic.estore.common.model.order.OrderShipment"
            not-null="false"
			fetch="select"
		>	
			<column name="orderShipmentId" />
		</many-to-one>
 <!-- exported bi-directional one-to-many association to OrderReturnSku -->
	<set name="orderReturnSkus"
	     table="order_return_sku"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="orderSkuId"/>
		</key>
		<one-to-many
			class="com.cartmatic.estore.common.model.order.OrderReturnSku"
		/>
	</set>
	<set name="purchaseOrderItems"
	     table="purchase_order_item"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="orderSkuId"/>
		</key>
		<one-to-many
			class="com.cartmatic.estore.common.model.supplier.PurchaseOrderItem"
		/>
	</set>
    </class>
</hibernate-mapping>
