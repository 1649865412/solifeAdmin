<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.cartmatic.estore.common.model.order.OrderShipment"
        table="order_shipment"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="orderShipmentId"
            column="orderShipmentId"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
 		<version
            name="version"
            type="java.lang.Integer"
            column="version"
            access="property"
            unsaved-value="undefined"
        />
        <property
            name="shipmentNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="shipmentNo"
            length="32"
            not-null="true"
            unique="false"
        />
        <property
            name="trackingNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="trackingNo"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="carrierName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="carrierName"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="itemSubTotal"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="itemSubTotal"
            precision="12"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="itemTax"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="itemTax"
            precision="12"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="shippingTax"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="shippingTax"
            precision="12"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="shippingCost"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="shippingCost"
            precision="12"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="shippingCostPaid"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="shippingCostPaid"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="discountAmount"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="discountAmount"
            precision="12"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="wrapName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="wrapName"
            length="64"
            not-null="false"
            unique="false"
        />
        <property
            name="wrapUnitPrice"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="wrapUnitPrice"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="wrapNote"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="wrapNote"
            length="512"
            not-null="false"
            unique="false"
        />
        <property
            name="hasRobotReviewed"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="hasRobotReviewed"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="isConfirmedByRobot"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isConfirmedByRobot"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="status"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="status"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="deliverTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="deliverTime"
            length="0"
            not-null="false"
            unique="false"
        />
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createTime"
            length="0"
            not-null="true"
            unique="false"
        />
        <property
            name="updateTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="updateTime"
            length="0"
            not-null="true"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to SalesOrder -->
		<many-to-one
			name="salesOrder"
			class="com.cartmatic.estore.common.model.order.SalesOrder"
            not-null="false"
			fetch="select"
		>	
			<column name="salesOrderId" />
		</many-to-one>
    <!-- imported bi-directional many-to-one association to OrderAddress -->
		<many-to-one
			name="orderAddress"
			class="com.cartmatic.estore.common.model.order.OrderAddress"
            not-null="false"
			fetch="select"
		>	
			<column name="shippingAddressId" />
		</many-to-one>
    <!-- imported bi-directional many-to-one association to OrderPickList -->
		<many-to-one
			name="orderPickList"
			class="com.cartmatic.estore.common.model.order.OrderPickList"
            not-null="false"
			fetch="select"
		>	
			<column name="orderPickListId" />
		</many-to-one>
		
		<many-to-one
			name="shippingRate"
			class="com.cartmatic.estore.common.model.system.ShippingRate"
            not-null="false"
			fetch="select"
		>	
			<column name="shippingRateId" />
		</many-to-one>
 <!-- exported bi-directional one-to-many association to OrderSku -->
	<set name="orderSkus"
	     table="order_sku"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	     order-by="orderSkuId"
	>
		<key>
			<column name="orderShipmentId"/>
		</key>
		<one-to-many
			class="com.cartmatic.estore.common.model.order.OrderSku"
		/>
	</set>     
    </class>
</hibernate-mapping>
