<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.cartmatic.estore.common.model.order.OrderSettlement"
        table="order_settlement"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="orderSettlementId"
            column="orderSettlementId"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
        <property
            name="shipmentId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="shipmentId"
            length="10"
            not-null="true"
            unique="false"
        />
         <property
            name="shipmentNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="shipmentNo"
            length="32"
            not-null="true"
            unique="false"
        />
        <property
            name="orderId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="orderId"
            length="10"
            not-null="true"
            unique="false"
        />
        <property
            name="orderNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="orderNo"
            length="20"
            not-null="true"
            unique="false"
        />
        <property
            name="carrierName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="carrierName"
            length="128"
            not-null="true"
            unique="false"
        />
        <property
            name="trackingNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="trackingNo"
            length="128"
            not-null="true"
            unique="false"
        />
        <property
            name="originalTotal"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="originalTotal"
            precision="12"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="settlementAmount"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="settlementAmount"
            precision="12"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="isComplete"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isComplete"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="addedBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="addedBy"
            length="32"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
