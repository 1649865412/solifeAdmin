            update="true"
            insert="true"
            access="property"
            column="zip"
            length="16"
            not-null="false"
            unique="false"
        />
        <property
            name="phone"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="phone"
            length="32"
            not-null="false"
            unique="false"
