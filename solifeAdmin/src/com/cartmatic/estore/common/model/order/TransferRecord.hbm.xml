            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="address"
            length="256"
            not-null="false"
            unique="false"
        />
        <property
            name="country"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="country"
            length="64"
            not-null="false"
