<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.cartmatic.estore.common.model.supplier.PurchaseOrder"
        table="purchase_order"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="purchaseOrderId"
            column="purchaseOrderId"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
 		<version
            name="version"
            type="java.lang.Integer"
            column="version"
            access="property"
            unsaved-value="undefined"
        />
        <property
            name="supplierName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="supplierName"
            length="32"
            not-null="true"
            unique="false"
        />
        <property
            name="purchaseOrderNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="purchaseOrderNo"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="trackingNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="trackingNo"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="poStatus"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="poStatus"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="poResult"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="poResult"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="createBy"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="createBy"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="updateBy"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="updateBy"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="updateTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="updateTime"
            length="0"
            not-null="true"
            unique="false"
        />
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createTime"
            length="0"
            not-null="true"
            unique="false"
        />
        <property
            name="comments"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="comments"
            length="512"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to Supplier -->
		<many-to-one
			name="supplier"
			class="com.cartmatic.estore.common.model.supplier.Supplier"
            not-null="false"
			fetch="select"
		>	
			<column name="supplierId" />
		</many-to-one>
 <!-- exported bi-directional one-to-many association to PurchaseOrderItem -->
	<set name="purchaseOrderItems"
	     table="purchase_order_item"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     order-by="productName"
	>
		<key>
			<column name="purchaseOrderId"/>
		</key>
		<one-to-many
			class="com.cartmatic.estore.common.model.supplier.PurchaseOrderItem"
		/>
	</set>     
    </class>
</hibernate-mapping>
