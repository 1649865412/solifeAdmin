<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.cartmatic.estore.common.model.supplier.SupplierProduct"
        table="supplier_product"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="supplierProductId"
            column="supplierProductId"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
 		<version
            name="version"
            type="java.lang.Integer"
            column="version"
            access="property"
            unsaved-value="undefined"
        />
        <property
            name="productName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="productName"
            length="256"
            not-null="true"
            unique="false"
        />
        <property
            name="productDesc"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="productDesc"
            length="65535"
            not-null="false"
            unique="false"
        />
        <property
            name="wholesalePrice"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="wholesalePrice"
            length="256"
            not-null="true"
            unique="false"
        />
        <property
            name="productCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="productCode"
            length="32"
            not-null="true"
            unique="false"
        />
        <property
            name="status"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="status"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="mediaUrl"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="mediaUrl"
            length="512"
            not-null="false"
            unique="false"
        />
        <property
            name="modifyLogs"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="modifyLogs"
            length="65535"
            not-null="false"
            unique="false"
        />
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createTime"
            length="0"
            not-null="true"
            unique="false"
        />
        <property
            name="updateTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="updateTime"
            length="0"
            not-null="true"
            unique="false"
        />
        <property
            name="createBy"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="createBy"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="updateBy"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="updateBy"
            length="10"
            not-null="false"
            unique="false"
        />
       
        <property
            name="tbCId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="tbCId"
            length="13"
            not-null="false"
            unique="false"
        />
         <property
            name="tbCatProps"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="tbCatProps"
            length="1024"
            not-null="false"
            unique="false"
        />
         <property
            name="tbSellCatProps"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="tbSellCatProps"
            length="65535"
            not-null="false"
            unique="false"
        />
        
        <property
            name="tbId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="tbId"
            length="13"
            not-null="false"
            unique="false"
        />
        
        <property
            name="updateLogs"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="updateLogs"
            length="512"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to Product -->
		<many-to-one
			name="product"
			class="com.cartmatic.estore.common.model.catalog.Product"
            not-null="false"
			fetch="select"
		>	
			<column name="productId" />
		</many-to-one>
    <!-- imported bi-directional many-to-one association to Supplier -->
		<many-to-one
			name="supplier"
			class="com.cartmatic.estore.common.model.supplier.Supplier"
            not-null="false"
			fetch="select"
		>	
			<column name="supplierId" />
		</many-to-one>
		<many-to-one name="category" class="com.cartmatic.estore.common.model.catalog.Category" not-null="false" fetch="select">
			<column name="categoryId" />
		</many-to-one>
    </class>
</hibernate-mapping>
