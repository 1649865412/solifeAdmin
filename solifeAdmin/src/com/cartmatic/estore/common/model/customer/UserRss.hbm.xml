<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.cartmatic.estore.common.model.customer.UserRss"
		table="user_rss" >
		
		<id name="id" column="id" type="java.lang.Integer"
			length="10">
			<generator class="native" />
		</id>
		
		<property name="userName" type="java.lang.String" update="true"
			insert="true" access="property" column="userName" length="255"/>
		
		<property name="email" type="java.lang.String" update="true"
			insert="true" access="property" column="email" length="255"/>
			
		<property name="status" type="java.lang.Integer" update="true"
			insert="true" access="property" column="status" /> 

	</class>
</hibernate-mapping>
