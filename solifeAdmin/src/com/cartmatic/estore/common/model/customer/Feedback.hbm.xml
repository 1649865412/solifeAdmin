<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.cartmatic.estore.common.model.customer.Feedback"
        table="FEEDBACK"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="feedbackId"
            column="feedbackId"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
 		<version
            name="version"
            type="java.lang.Integer"
            column="version"
            access="property"
            unsaved-value="undefined"
        />
        <property
            name="title"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="title"
            length="8"
            not-null="false"
            unique="false"
        />
        <property
            name="firstName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="firstName"
            length="32"
            not-null="false"
            unique="false"
        />
        <property
            name="lastName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="lastName"
            length="32"
            not-null="false"
            unique="false"
        />
        <property
            name="subject"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="subject"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="content"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="content"
            length="512"
            not-null="false"
            unique="false"
        />
        <property
            name="status"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="status"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="replyType"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="replyType"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="feedbackType"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="feedbackType"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="priority"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="priority"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="email"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="email"
            length="64"
            not-null="false"
            unique="false"
        />
        <property
            name="telephone"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="telephone"
            length="32"
            not-null="false"
            unique="false"
        />
        <property
            name="fax"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="fax"
            length="32"
            not-null="false"
            unique="false"
        />
        <property
            name="threadId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="threadId"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="updateTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="updateTime"
            length="0"
            not-null="false"
            unique="false"
        />
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createTime"
            length="0"
            not-null="false"
            unique="false"
        />
        <property
            name="givenShopPointAction"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="givenShopPointAction"
            length="5"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to AppUser -->
		<many-to-one
			name="appUser"
			class="com.cartmatic.estore.common.model.system.AppUser"
            not-null="false"
			fetch="select"
		>	
			<column name="appuserId" />
		</many-to-one>
    <!-- imported bi-directional many-to-one association to Product -->
		<many-to-one
			name="product"
			class="com.cartmatic.estore.common.model.catalog.Product"
            not-null="false"
			fetch="select"
		>	
			<column name="productId" />
		</many-to-one>
    </class>
</hibernate-mapping>
