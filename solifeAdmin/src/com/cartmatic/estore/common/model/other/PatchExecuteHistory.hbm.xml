<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.cartmatic.estore.common.model.other.PatchExecuteHistory"
        table="PATCH_EXECUTE_HISTORY"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="patchExecuteHistoryId"
            column="patchExecuteHistoryId"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
 		<version
            name="version"
            type="java.lang.Integer"
            column="version"
            access="property"
            unsaved-value="undefined"
        />
        <property
            name="transactionUid"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="transactionUid"
            length="32"
            not-null="false"
            unique="false"
        />
        <property
            name="fileName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="fileName"
            length="64"
            not-null="true"
            unique="false"
        />
        <property
            name="fileType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="fileType"
            length="64"
            not-null="true"
            unique="false"
        />
        <property
            name="flag"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="flag"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="resultDescription"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="resultDescription"
            length="256"
            not-null="false"
            unique="false"
        />
        <property
            name="fromVersion"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="fromVersion"
            length="32"
            not-null="true"
            unique="false"
        />
        <property
            name="toVersion"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="toVersion"
            length="32"
            not-null="true"
            unique="false"
        />
        <property
            name="executeTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="executeTime"
            length="0"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
