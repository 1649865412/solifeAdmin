<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.cartmatic.estore.common.model.system.PaymentHistory"
        table="PAYMENT_HISTORY"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="paymentHistoryId"
            column="paymentHistoryId"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
 		<version
            name="version"
            type="java.lang.Integer"
            column="version"
            access="property"
            unsaved-value="undefined"
        />
        <property
            name="orderNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="orderNo"
            length="20"
            not-null="true"
            unique="false"
        />
        <property
            name="flag"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="flag"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="flowNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="flowNo"
            length="64"
            not-null="false"
            unique="false"
        />
        <property
            name="amount"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="amount"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="errorMessage"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="errorMessage"
            length="256"
            not-null="false"
            unique="false"
        />
        <property
            name="errorCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="errorCode"
            length="32"
            not-null="false"
            unique="false"
        />
        <property
            name="remoteIp"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remoteIp"
            length="32"
            not-null="false"
            unique="false"
        />
        <property
            name="receiveData"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="receiveData"
            length="2048"
            not-null="false"
            unique="false"
        />
        <property
            name="paymentMethodId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="paymentMethodId"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="isBrowsed"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isBrowsed"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createTime"
            length="0"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
