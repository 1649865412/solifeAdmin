<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.cartmatic.estore.common.model.system.SystemVersion"
        table="SYSTEM_VERSION"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="systemVersionId"
            column="systemVersionId"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
 		<version
            name="version"
            type="java.lang.Integer"
            column="version"
            access="property"
            unsaved-value="undefined"
        />
        <property
            name="sysVersion"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="sysVersion"
            length="32"
            not-null="true"
            unique="false"
        />
        <property
            name="productCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="productCode"
            length="64"
            not-null="true"
            unique="false"
        />
        <property
            name="domainName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="domainName"
            length="256"
            not-null="true"
            unique="false"
        />
        <property
            name="licenseKey"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="licenseKey"
            length="32"
            not-null="true"
            unique="false"
        />
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createTime"
            length="0"
            not-null="true"
            unique="false"
        />
        <property
            name="updateTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="updateTime"
            length="0"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
