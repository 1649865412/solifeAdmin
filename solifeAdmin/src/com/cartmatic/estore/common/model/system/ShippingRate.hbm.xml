<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.cartmatic.estore.common.model.system.ShippingRate"
        table="SHIPPING_RATE"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
		where="deleted=0"
    >
		<id
            name="shippingRateId"
            column="shippingRateId"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
 		<version
            name="version"
            type="java.lang.Integer"
            column="version"
            access="property"
            unsaved-value="undefined"
        />
        <property
            name="baseOn"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="baseOn"
            length="10"
            not-null="true"
            unique="false"
        />
        <property
            name="isFlat"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isFlat"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="isRoundUpValue"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isRoundUpValue"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="basePrice"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="basePrice"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="maxWeight"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="maxWeight"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="maxVolume"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="maxVolume"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="baseWeight"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="baseWeight"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="baseVolume"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="baseVolume"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="weightPerRate"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="weightPerRate"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="volumePerRate"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="volumePerRate"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="volumePerFee"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="volumePerFee"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="increaseUnit"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="increaseUnit"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="metricUnitCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="metricUnitCode"
            length="32"
            not-null="false"
            unique="false"
        />
        <property
            name="maxItem"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="maxItem"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="itemPerRate"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="itemPerRate"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="description"
            length="1024"
            not-null="false"
            unique="false"
        />
        <property
            name="deleted"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="deleted"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="minWeight"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="minWeight"
            precision="12"
            scale="2"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to Region -->
		<many-to-one
			name="region"
			class="com.cartmatic.estore.common.model.system.Region"
            not-null="false"
			fetch="select"
		>	
			<column name="regionId" />
		</many-to-one>
    <!-- imported bi-directional many-to-one association to ShippingMethod -->
		<many-to-one
			name="shippingMethod"
			class="com.cartmatic.estore.common.model.system.ShippingMethod"
            not-null="false"
			fetch="select"
		>	
			<column name="shippingMethodId" />
		</many-to-one>
		<sql-delete>UPDATE SHIPPING_RATE SET deleted=1 WHERE shippingRateId=? and version=?</sql-delete>
    </class>
</hibernate-mapping>
