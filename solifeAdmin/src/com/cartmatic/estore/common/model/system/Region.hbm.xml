<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.cartmatic.estore.common.model.system.Region"
        table="REGION"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="regionId"
            column="regionId"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
 		<version
            name="version"
            type="java.lang.Integer"
            column="version"
            access="property"
            unsaved-value="undefined"
        />
        <property
            name="regionCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="regionCode"
            length="32"
            not-null="true"
            unique="false"
        />
        <property
            name="regionType"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="regionType"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="regionName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="regionName"
            length="64"
            not-null="true"
            unique="false"
        />
        <property
            name="zipCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="zipCode"
            length="12"
            not-null="false"
            unique="false"
        />
        <property
            name="telephoneCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="telephoneCode"
            length="8"
            not-null="false"
            unique="false"
        />
        <property
            name="regionIcon"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="regionIcon"
            length="256"
            not-null="false"
            unique="false"
        />
        <property
            name="priority"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="priority"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="parentRegionId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="parentRegionId"
            length="10"
            not-null="true"
            unique="false"
        />
        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="description"
            length="1024"
            not-null="false"
            unique="false"
        />
        <property
            name="status"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="status"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="createBy"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="createBy"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createTime"
            length="0"
            not-null="false"
            unique="false"
        />
        <property
            name="updateBy"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="updateBy"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="updateTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="updateTime"
            length="0"
            not-null="false"
            unique="false"
        />
  <!-- exported bi-directional one-to-many association to RegionItem -->
	<set name="regionItems"
	     table="region_item"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="regionId"/>
		</key>
		<one-to-many
			class="com.cartmatic.estore.common.model.system.RegionItem"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to ShippingRate -->
	<set name="shippingRates"
	     table="shipping_rate"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="regionId"/>
		</key>
		<one-to-many
			class="com.cartmatic.estore.common.model.system.ShippingRate"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to TaxRate -->
	<set name="taxRates"
	     table="tax_rate"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="regionId"/>
		</key>
		<one-to-many
			class="com.cartmatic.estore.common.model.system.TaxRate"
		/>
	</set>     
    </class>
</hibernate-mapping>
