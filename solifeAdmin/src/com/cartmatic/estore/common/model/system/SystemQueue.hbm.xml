<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.cartmatic.estore.common.model.system.SystemQueue"
        table="SYSTEM_QUEUE"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="systemQueueId"
            column="systemQueueId"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
         <property
            name="title"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="title"
            length="256"
            not-null="true"
            unique="false"
        />
        <property
            name="queueType"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="queueType"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="execTimes"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="execTimes"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="targetEntiy"
            type="org.springframework.orm.hibernate3.support.BlobSerializableType"
            update="true"
            insert="true"
            access="property"
            column="targetEntiy"
            not-null="false"
            unique="false"
        />
        <property
            name="errorMsg"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="errorMsg"
            length="1024"
            not-null="false"
            unique="false"
        />
        <property
            name="queueStatus"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="queueStatus"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createTime"
            length="0"
            not-null="true"
            unique="false"
        />
        <property
            name="updateTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="updateTime"
            length="0"
            not-null="true"
            unique="false"
        />
        <property
            name="nextRetryTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="nextRetryTime"
            length="0"
            not-null="true"
            unique="false"
        />
     </class>
</hibernate-mapping>
